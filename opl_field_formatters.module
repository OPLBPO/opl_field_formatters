        $tid = $item['target_id'];
        $term = taxonomy_term_load($tid);
        $translated_term = i18n_taxonomy_localize_terms($term);

        $display_terms[] = l(
            $translated_term->name,
            'program',
            array('query' => array('f[' . $delta . ']'=>'taxonomy_vocabulary_21:'.$tid
